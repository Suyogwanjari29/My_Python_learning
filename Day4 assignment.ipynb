    "### 12. Take integer from user and print all odd numbers which can be generated using any number of digits from given number
     ### Ex. 
     ### Input: 3421  o/p: 1,3,13,43,23,31,41,21,421,241,341,431,423,243,143,413,1243,1423,4213,4123, 2413,2143, .. etc"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "76431651-1123-4fdd-8222-98876ddcf092",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter number 3421\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "odd :  [3, 1]\n",
      "even :  [4, 2]\n"
     ]
    }
   ],
   "source": [
    "num = input(\"Enter number\")\n",
    "l1 = []\n",
    "odd = []\n",
    "even = []\n",
    "for i in num:\n",
    "    i = int(i)\n",
    "    if(i%2 != 0):\n",
    "        odd.append(i)\n",
    "    else:\n",
    "        even.append(i)\n",
    "print(\"odd : \",odd)\n",
    "print(\"even : \",even)\n"
   ]
  },
